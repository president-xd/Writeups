<svg
  xmlns="http://www.w3.org/2000/svg"
  version="1.1"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  viewBox="<%= x %> <%= y %> <%= width %> <%= height %>"
  width="<%= width %>"
  height="<%= height %>"
>
  <% for (let p of points) { %>
  <polyline points="<%= p %>" fill="transparent" stroke-width="3pt" stroke="black"></polyline>
  <% } %>
</svg>
